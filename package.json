{
  "name": "react-oas-integration",
  "version": "2.0.0",
  "description": "React OAS Integration System - A modern web application designed to integrate OneAutomationSystem with Google Sheets",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.20",
    "@mui/x-data-grid": "^6.18.2",
    "@reduxjs/toolkit": "^1.9.7",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "ajv": "^8.17.1",
    "axios": "^1.6.2",
    "chart.js": "^4.5.0",
    "framer-motion": "^10.16.16",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-query": "^3.39.3",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.20.1",
    "react-scripts": "5.0.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "styled-components": "^6.1.6",
    "typescript": "^5.3.2",
    "web-vitals": "^3.5.0",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "@types/styled-components": "^5.1.32",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "react-scripts start",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:watch": "react-scripts test --watch",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint src/**/*.{js,jsx,ts,tsx} --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md}",
    "clean": "rm -rf build dist node_modules/.cache",
    "dev:full": "concurrently \"npm run dev\" \"npm run backend:dev\" \"npm run automation:dev\"",
    "backend:install": "cd backend && npm install",
    "backend:dev": "cd backend && npm run dev",
    "backend:build": "cd backend && npm run build",
    "backend:start": "cd backend && npm start",
    "automation:install": "cd automation && pip install -r requirements.txt",
    "automation:dev": "cd automation && python src/main.py",
    "automation:test": "cd automation && python -m pytest",
    "install:all": "npm install && npm run backend:install && npm run automation:install",
    "build:all": "npm run build && npm run backend:build",
    "test:all": "npm run test:coverage && npm run backend:test && npm run automation:test",
    "start:prod": "concurrently \"npm run preview\" \"npm run backend:start\"",
    "preview": "npx serve -s build -l 3000",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "deploy:vercel": "vercel --prod",
    "deploy:prepare": "npm run build:all && npm run test:all"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ],
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "react",
    "typescript",
    "google-sheets",
    "oauth",
    "material-ui",
    "redux",
    "automation",
    "oas-integration"
  ],
  "author": "React OAS Integration Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/react-oas-integration.git"
  },
  "homepage": "https://react-oas-integration.vercel.app"
}
